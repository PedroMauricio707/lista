	p = i; 	            	//  Há problema( porém o programa ainda será compilado).
	q = &j;		           //  Não há problema.
	p = &*&i;	           //  Não há problema.
	i = (*&)j;	         //  Há problema( ilegal).
	i = *&j;	           //  Não há problema.
	i = *&*&j;	         //  Não há problema.
	q = *p;		           //  Há problema ( porém o programa ainda será compilado).
	i = (*p)++ + *q;     //  Não há problema.
